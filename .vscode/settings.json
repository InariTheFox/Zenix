{
    "C_Cpp.default.includePath": [
        "include",
        "include/sys",
        "arch/z180/include"
    ],
    "C_Cpp.default.configurationProvider": "ms-vscode.makefile-tools",
    "files.associations": {
        "*.h": "c",
        "*.asm": "retroasm_z80",
        "*.s": "retroasm_z80",
        "*.c": "c",
        "cstdlib": "c",
        "locale": "c",
        "ostream": "c",
        "new": "c"
    },
    "workbench.editorAssociations": {
        "*.bin": "hexEditor.hexedit"
    },
    "C_Cpp.errorSquiggles": "enabled"
}